<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="group23.pacman.view.EditNameController">
   <children>
      <ImageView fx:id="background" fitHeight="200.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true" />
      <TextField fx:id="nameField" layoutX="150.0" layoutY="60.0" onKeyPressed="#handleEnter" prefHeight="25.0" prefWidth="200.0" />
      <ImageView fx:id="errorMessage" fitHeight="80.0" fitWidth="450.0" layoutX="25.0" layoutY="100.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
